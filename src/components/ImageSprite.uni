<template>
    <div class="sprite image-sprite">
        <script name="uni">
            var img = this.find("img");
            img.src = this.props.src;
            img.setAttribute("usemap", "#map");
            if (this.props.subclass){
                this.props.subclass.split(" ").forEach(cl => {
                    this.classList.add(cl);
                })
            }
        </script>
        <img height="100%">
    </div>
</template>